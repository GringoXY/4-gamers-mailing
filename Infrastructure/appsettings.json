{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "BackgroundServices": {
    "OutboxMessagesConsumer": {
      "RabbitMQ": {
        "HostName": "localhost",
        "Port": 5672,
        "UserName": "user",
        "QueueName": "outbox-messages"
      }
    },
    "SendEmailInboxMessages": {
      "Interval": "0.00:01:00",
      "Smtp": {
        "DisplayName": "4Gamers robot",
        "Host": "SmtpHost",
        "Port": 587,
        "Username": "email@example.com",
        "EnableSsl": true
      }
    }
  },
  "PostgreSQL": {
    "Database": "4Gamers",
    "Host": "localhost",
    "Port": 5432,
    "Username": "postgres"
  },
  "DocsApi": {
    "Scheme": "http",
    "Host": "localhost",
    "Port": 5287
  }
}
